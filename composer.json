{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {"type": "path", "url": "/mnt/fat/php/projects/codex/workbench/laradic/*"},
        {"type": "path", "url": "codex/*"}
    ],
    "replace": {
        "codex/core": "*"
    },
    "require": {
        "php": "^7.1.3",
        "codex/algolia-search": "~2.0",
        "codex/auth": "~2.0",
        "codex/blog": "~2.0",
        "codex/comments": "~2.0",
        "codex/filesystems": "~2.0",
        "codex/git": "~2.0",
        "codex/packagist": "~2.0",
        "codex/phpdoc": "~2.0",
        "codex/sitemap": "~2.0",
        "colinodell/json5": "^2.0",
        "czproject/git-php": "^3.17",
        "doctrine/dbal": "^2.9",
        "eddiriarte/console-select": "^2.0",
        "fideloper/proxy": "^4.0",
        "gitonomy/gitlib": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "phpextra/sorter": "^1.0",
        "spatie/laravel-responsecache": "^4.4",
        "teqneers/php-stream-wrapper-for-git": "^2.0",
        "timacdonald/rule-builder": "^4.0",
        "zendframework/zend-config": "3.2.*",
        "zendframework/zend-config-aggregator": "1.1.*",
        "zendframework/zend-config-aggregator-parameters": "1.1.*",
        "zumba/json-serializer": "^2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "wikimedia/composer-merge-plugin": "1.4.*",
        "barryvdh/laravel-ide-helper": "^2.5",
        "barryvdh/laravel-debugbar": "~3.2",
        "laravel/telescope": "~1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "codex/core/composer.json"
            ]
        },
        "laravel": {
            "dont-discover": [
                "codex/core",
                "nuwave/lighthouse",
                "deinternetjongens/lighthouse-utils"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "ide": [
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate",
            "@php artisan ide:toolbox"
        ],
        "satis:install": [
            "cd .tmp",
            "rm -rf satis",
            "@composer create-project composer/satis:dev-master --ansi"
        ],
        "satis:build": [
            "rm -rf .tmp/satis/cache",
            "rm -rf .tmp/satis/public",
            "@php .tmp/satis/bin/satis build --stats -vvv --ansi"
        ],
        "satis:serve": [
            "@php -S localhost:8999 -t .tmp/satis/public"
        ],
        "satis": [
            "@composer satis:build",
            "@composer satis:serve"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
